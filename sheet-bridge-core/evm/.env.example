# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Token address to be locked in the contract
TOKEN_ADDRESS=0x0000000000000000000000000000000000000000

# Owner address for the TokenLock contract (optional, defaults to deployer)
OWNER_ADDRESS=0x0000000000000000000000000000000000000000

# BSC RPC URLs (optional, will use public endpoints if not set)
BSC_RPC_URL=https://bsc-dataseed1.binance.org
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# BSCScan API key for contract verification
BSCSCAN_API_KEY=your_bscscan_api_key_here

# Gas reporting (set to true to enable)
REPORT_GAS=false

# CoinMarketCap API key for gas reporting in USD
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
